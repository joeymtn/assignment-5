{
  "name": "cse183-assignment5",
  "version": "1.0.0",
  "description": "CSE183 Assignment 5 - React II",
  "author": "David Harrison <dcharris@ucsc.edu>",
  "license": "UNLICENSED",
  "repository": "none",
  "dependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "bestzip": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jest-dom": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "eslint-plugin-testing-library": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --coverage",
    "lint": "eslint src/**/*.js",
    "zip": "bestzip CSE183-Assignment5-Submission.zip src/*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/__tests__/common.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
